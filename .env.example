# dTRINITY Katana Contracts Environment Variables
# Copy this file to .env and fill in your values

# Katana Network Information
# Mainnet: Chain ID 747474, RPC: https://rpc.katana.network/
# Testnet (Bokuto): Chain ID 737373, RPC: https://rpc-bokuto.katanarpc.com
# Block Explorers: https://katanascan.com/ (mainnet), https://bokuto.katanascan.com/ (testnet)
# Faucet: https://faucet.katana.tools/ (testnet)

# Katana API Key for block explorer verification
KATANA_API_KEY=your_katana_api_key_here

# Katana Mainnet (Chain ID: 747474) Deployer Credentials
# SECURITY WARNING: Never commit real mainnet mnemonics/keys to version control
MNEMONIC_MAINNET_DEPLOYER=your_mainnet_mnemonic_here
PK_MAINNET_DEPLOYER=your_mainnet_private_key_here

# Katana Testnet Bokuto (Chain ID: 737373) Deployer Credentials
# Use testnet faucet: https://faucet.katana.tools/
MNEMONIC_TESTNET_DEPLOYER=your_testnet_mnemonic_here
PK_TESTNET_DEPLOYER=your_testnet_private_key_here

# Safe Protocol Configuration
# Set to 'true' to force Safe multisig mode for testing on non-mainnet networks
USE_SAFE=false

# Enable IR compilation for all contracts (slower but avoids stack too deep errors)
VIA_IR=false